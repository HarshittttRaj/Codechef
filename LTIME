#include <iostream>

using namespace std;

int main() {
    int n;
    cin>>n;
    int arr[n];
     for(int i=0; i<n; i++){
         cin>>arr[i];
     }
     
     for(int x=0; x<n; x++){
         if(arr[x]>=1 && arr[x]<=4){
             cout<<"YES"<<endl;
         }
         else{
             cout<<"NO"<<endl;
         }
     }
	return 0;
}
